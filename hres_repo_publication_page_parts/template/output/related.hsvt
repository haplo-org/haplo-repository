
std:web-publisher:template("hres:repository:common:ui:panel") {
  if(related.length) {
    each(related) {
      <p class="haplo-repository-listing-citation">
        <a href=href> output.title </a>
        if(citation) { <br> <small> render(citation)</small> }
      </p>
    }
  } else {
    <p>"No related outputs"</p>
  }
} heading { "Related outputs" }
