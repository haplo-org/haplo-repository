
std:web-publisher:template("hres:repository:common:ui:panel") {
  if(items.length) {
    each(items) {
      <p>
        <a href=href> output.title </a>
        if(citation) { <br> <small><cite> render(citation) </cite></small> }
      </p>
    }
  } else {
    <p>"No items in this collection."</p>
  }
} heading { title }
