
each(embargoes) {
  <small class="text-uppercase"><mark>
    "Under embargo "
    if(end) {
      "until " std:date(end)
    } else {
      "indefinitely"
    }
    if(multipleFiles) {
      <em> " (" multipleFiles " files)" </em>
    }
    if(licenseURL) { " terms available at: " <a href=licenseURL class="text-lowercase"> licenseURL </a> }
  <br>
  </mark></small>
}
